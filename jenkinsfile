pipeline {
  agent any {
   
   stages {
   stage('Getting the project from GIT') {
            steps {
               echo 'Pulling..';
                git branch: 'main',
                url: 'https://github.com/KrishnaveniVadlamudi/JenkinsDemo.git';
            }
        }
   
      stage('Build') {
        sh 'mvn -Dmaven.test.failure.ignore=true install' 
        withMaven {
      sh "mvn clean verify"
    } // w
 
      }
      stage('Test'){
       sh 'mvn clean package'
       sh 'package'
      }
    }
  }
}
