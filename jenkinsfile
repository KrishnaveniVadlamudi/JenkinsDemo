pipeline {
  agent any {
   tools { 
        maven 'MAVEN3.8.7' 
        jdk 'JAVA1.7' 
    }
   stages {
     stage('Build') {
         git 'https://github.com/KrishnaveniVadlamudi/JenkinsDemo.git'
         sh 'mvn -Dmaven.test.failure.ignore=true install' 

      }
      stage('Test'){
       sh 'mvn clean build'
      }
    }
  }
}
